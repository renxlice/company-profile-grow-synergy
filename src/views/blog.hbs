    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Category filter functionality
            const categoryBtns = document.querySelectorAll('.category-btn');
            const searchInput = document.getElementById('searchInput');
            
            // Load more functionality
            let currentPage = 1;
            const postsPerPage = 6;
            let allBlogPosts = Array.from(document.querySelectorAll('#blogGrid article'));
            
            // Initially show only first page
            function showPosts(page, postsToShow = allBlogPosts) {
                const startIndex = (page - 1) * postsPerPage;
                const endIndex = startIndex + postsPerPage;
                
                postsToShow.forEach((post, index) => {
                    if (index >= startIndex && index < endIndex) {
                        post.style.display = 'block';
                    } else {
                        post.style.display = 'none';
                    }
                });
                
                // Update button state
                const loadMoreBtn = document.getElementById('loadMoreBtn');
                const remainingPosts = postsToShow.length - endIndex;
                
                if (remainingPosts <= 0) {
                    loadMoreBtn.textContent = 'Tidak Ada Artikel Lagi';
                    loadMoreBtn.disabled = true;
                    loadMoreBtn.classList.add('opacity-50', 'cursor-not-allowed');
                } else {
                    loadMoreBtn.textContent = `Muat Lebih Banyak (${remainingPosts} artikel tersisa)`;
                    loadMoreBtn.disabled = false;
                    loadMoreBtn.classList.remove('opacity-50', 'cursor-not-allowed');
                }
            }
            
            // Filter posts function
            function filterPosts(category = 'all', searchTerm = '') {
                currentPage = 1;
                const filteredPosts = Array.from(allBlogPosts).filter(post => {
                    const title = post.querySelector('h2').textContent.toLowerCase();
                    const description = post.querySelector('p').textContent.toLowerCase();
                    const postCategory = post.dataset.category;
                    
                    const matchesCategory = category === 'all' || postCategory === category;
                    const matchesSearch = title.includes(searchTerm) || description.includes(searchTerm);
                    
                    return matchesCategory && matchesSearch;
                });
                
                showPosts(currentPage, filteredPosts);
                return filteredPosts;
            }
            
            // Category button clicks
            categoryBtns.forEach(btn => {
                btn.addEventListener('click', function() {
                    // Update active button styling
                    categoryBtns.forEach(b => {
                        b.classList.remove('bg-orange-500', 'text-white');
                        b.classList.add('bg-gray-200', 'text-gray-700');
                    });
                    this.classList.remove('bg-gray-200', 'text-gray-700');
                    this.classList.add('bg-orange-500', 'text-white');
                    
                    const category = this.dataset.category;
                    const searchTerm = searchInput.value.toLowerCase();
                    filterPosts(category, searchTerm);
                });
            });
            
            // Search functionality
            searchInput.addEventListener('input', function() {
                const searchTerm = this.value.toLowerCase();
                const activeCategory = document.querySelector('.category-btn.bg-orange-500')?.dataset.category || 'all';
                filterPosts(activeCategory, searchTerm);
            });
            
            // Load more button
            document.getElementById('loadMoreBtn').addEventListener('click', function() {
                currentPage++;
                const activeCategory = document.querySelector('.category-btn.bg-orange-500')?.dataset.category || 'all';
                const searchTerm = searchInput.value.toLowerCase();
                
                const filteredPosts = Array.from(allBlogPosts).filter(post => {
                    const title = post.querySelector('h2').textContent.toLowerCase();
                    const description = post.querySelector('p').textContent.toLowerCase();
                    const postCategory = post.dataset.category;
                    
                    const matchesCategory = activeCategory === 'all' || postCategory === activeCategory;
                    const matchesSearch = title.includes(searchTerm) || description.includes(searchTerm);
                    
                    return matchesCategory && matchesSearch;
                });
                
                const startIndex = (currentPage - 1) * postsPerPage;
                const endIndex = startIndex + postsPerPage;
                
                filteredPosts.forEach((post, index) => {
                    if (index >= startIndex && index < endIndex) {
                        post.style.display = 'block';
                    }
                });
                
                // Update button state
                const remainingPosts = filteredPosts.length - endIndex;
                if (remainingPosts <= 0) {
                    this.textContent = 'Tidak Ada Artikel Lagi';
                    this.disabled = true;
                    this.classList.add('opacity-50', 'cursor-not-allowed');
                } else {
                    this.textContent = `Muat Lebih Banyak (${remainingPosts} artikel tersisa)`;
                }
                
                // Scroll to newly loaded posts
                const newlyVisiblePosts = filteredPosts.filter((post, index) => {
                    return index >= startIndex && index < endIndex;
                });
                
                if (newlyVisiblePosts.length > 0) {
                    newlyVisiblePosts[0].scrollIntoView({ behavior: 'smooth', block: 'start' });
                }
            });
            
            // Initialize first page
            showPosts(currentPage);
        });
    </script>
    
