{
  "name": "company-profile-grow-synergy",
  "version": "1.0.0",
  "description": "Platform Pelatihan Data Analitik Terbaik di Indonesia",
  "main": "server.js",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "build": "npm install --force && ./node_modules/.bin/nest build && node scripts/build-assets.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node server.js",
    "start:dev": "set NODE_ENV=development && npx nest start --watch",
    "start:debug": "npx nest start --debug --watch",
    "start:prod": "node server.js",
    "start:silent": "node -r ./scripts/disable-console.js dist/src/main",
    "start:dev:silent": "node -r ./scripts/disable-console.js node_modules/.bin/nest start --watch",
    "start:prod:silent": "set NODE_ENV=production && node -r ./scripts/disable-console-env.js dist/src/main",
    "start:dev:env-silent": "set NODE_ENV=silent && node -r ./scripts/disable-console-env.js npx nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "setup:firestore": "node setup-firestore.js",
    "firebase:setup": "npm run setup:firestore",
    "setup:env": "node scripts/setup-env.js",
    "security:check": "node scripts/deploy-check.js",
    "seed:firestore": "echo 'Please add data via admin panel at http://localhost:3001/admin/login'",
    "ensure:admin": "node scripts/ensure-admin.js",
    "reset:admin": "node scripts/reset-admin-password.js",
    "check:admin": "node scripts/check-admin-password.js",
    "predeploy": "npm run security:check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renxlice/company-profile-grow-synergy.git"
  },
  "keywords": [
    "data analytics",
    "pelatihan data analitik",
    "kursus online",
    "data science",
    "nestjs",
    "seo",
    "indonesia"
  ],
  "author": "DataAnalitik.id",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-fastify": "^10.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/throttler": "^5.0.0",
    "@types/express-session": "^1.18.2",
    "@types/multer": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "firebase-admin": "^13.6.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.3",
    "method-override": "^3.0.0",
    "multer": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "validator": "^13.15.26",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
